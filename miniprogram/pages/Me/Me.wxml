<view class="page">
  <view class='headName'>
    <open-data class="head" type="userAvatarUrl"></open-data>
    <open-data class="name" type="userNickName" lang="zh_CN"></open-data>
  </view>
  <view class='grayLine1'></view>

  <view class='testHistory'>
    <view class='text'>姓名：</view>
    <view class='text' style='color:#343535;'>{{realName}}</view>
  </view>


  <view class='grayLine1'></view>

  <view class='testHistory'>
    <view class='text'>班级：</view>
    <view class='text' style='color:#343535;'>{{className}}</view>
  </view>

  <view class='grayLine1'></view>

  <view class='personalInformation'>
    <view class='text'>学号: </view>
    <view class='text' style='color:#343535;'>{{studentId}}</view>
  </view>

  <view class='grayLine1'></view>

  <view class='personalInformation'>
    <view class='text'>有效积分: </view>
    <view class='text' style='color:#343535;'>{{score}}</view>
  </view>

  <view class='grayLine1'></view>
</view>